<?xml version="1.0" encoding="UTF-8"?>
<aven>
	<register>
	    <!-- protocol can be: redis, zookeeper -->
	    <protocol>redis</protocol>
	    <address>127.0.0.1:6379</address>
	    
	    <!-- <address>127.0.0.1:2181</address>  -->
	    
	</register>
	
    <!-- protocol can be: tcp, http -->
    <bootstrap>http:9009</bootstrap>
    <bootstrap>tcp:9010</bootstrap>
    
    <client>tcp</client>
	
	<!-- remote ip list -->
	<remote>av.test.nado.TestRemote</remote>
	<remote>av.test.nado.TestOutput</remote>
	<remote>av.test.nado.TestRemoteTimer</remote>
	
</aven>
